{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.1.0"
    }
  },
  "routes": [
    { "src": "/api$,?", "dest": "/api/health" },
    { "src": "/api/admin/forms/(.*)", "dest": "/api/admin?path=/api/admin/forms/$1" },
    { "src": "/api/admin(?:/.*)?", "dest": "/api/admin" },
    { "src": "/api/reports(?:/.*)?", "dest": "/api/reports" }
  ]
}
