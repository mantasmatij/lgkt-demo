  "scripts": {
    "test": "npm run db:migrate && nx run-many -t test --exclude lgkt-forma",
    "build": "nx build web && nx build validation && nx build api && npx nx run api:prune",
    "test:cucumber": "cucumber-js -c web/tests/e2e/web.cucumber.mjs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "nx run-many -t lint --exclude lgkt-forma",
    "dev": "docker compose -f docker/docker-compose.yml up --build",
    "dev:up": "docker compose -f docker/docker-compose.yml up -d --build",
    "dev:down": "docker compose -f docker/docker-compose.yml down",
    "dev:destroy": "docker compose -f docker/docker-compose.yml down -v",
    "dev:api-local": "nx serve api -c development",
    "dev:web-local": "next dev web",
    "contracts:gen": "openapi-typescript specs/001-users-mantas-matijosaitis/contracts/openapi.yaml -o contracts/src/generated.ts",
    "db:generate": "drizzle-kit generate --config db/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config db/drizzle.config.ts",
    "db:reset:dev": "node db/scripts/reset-dev-db.js",
    "db:seed:dev": "node db/scripts/seed-sample-submissions.js",
    "db:seed:admin": "node db/scripts/seed-admin.js"
  },
