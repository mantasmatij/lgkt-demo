{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/vercel-handler.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "builds": [
    { "src": "api/vercel-handler.js", "use": "@vercel/node" },
    { "src": "web/package.json", "use": "@vercel/next" }
  ],
  "functions": {
    "api/vercel-handler.js": {
      "runtime": "@vercel/node@3.1.0",
      "maxDuration": 10
    }
  }
}